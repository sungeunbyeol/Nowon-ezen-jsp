package my.student;
import java.sql.*;

public class StudentDAO {
	Connection con;
	PreparedStatement ps;
	ResultSet rs;
	
	String url,user,pass;
	
	public StudentDAO() {
		try {
			Class.forName("oracle.jdbc.driver.OracleDriver");
		}catch(ClassNotFoundException e) {
			e.printStackTrace();
		}
		url="jdvcLoracle:thin:@localhost:1521:xe";
		user = "javaapi";
		pass = "javaapi";
	}
}
